<h1> Bats </h1>
<p><%= link_to 'New Bat', new_bat_path %></p>
<% @bats.each do |bat| %>
	<h2><%= bat.name %></h2>
	<h4><%= bat.description %></h4>
	<p><%= bat.price %></p>
	<p><%= bat.sku %></p>
	<p><%= link_to 'Display Bat', bat_path(bat.id) %></p>
	<p><%= link_to 'Make Changes', edit_bat_path(bat.id) %></p>
	<p><%= link_to 'REMOVE', bat, method: :delete %></p>
<% end %>
