<h1> New Bat </h1>
<%= form_for @bat do |f| %>
	<%= f.label :name %>
	<%= f.text_field :name %>

	<%= f.label :description %>
	<%= f.text_field :description %>

	<%= f.label :price %>
	<%= f.number_field :price, :class => 'text_field', :step => 'any' %>

	<%= f.label :sku %>
	<%= f.text_field :sku %>

	<%= f.submit %>
<% end %>



